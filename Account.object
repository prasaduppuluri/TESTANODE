        <formula>IF( Data_Quality_Score__c =100,&quot;All Account Details Captured&quot;, &quot;Missing: &quot;
&amp;IF( LEN ( Website) = 0, &quot;Website, &quot;,&quot;&quot;)&amp;&quot;&quot;&amp;
IF(  ISBLANK ( Waste_Spend_Total_Amount__c),&quot;Waste Spend Total Amount, &quot;,&quot;&quot;)&amp;&quot;&quot;&amp;
IF( ISBLANK ( AnnualRevenue),&quot;Annual Revenue, &quot;,&quot;&quot;)&amp;&quot;&quot;&amp;
IF( LEN ( Phone) = 0, &quot;Phone, &quot;,&quot;&quot;)&amp;&quot;&quot;&amp; 
IF( LEN ( BillingCity) = 0, &quot;Billing City, &quot;,&quot;&quot;)&amp;&quot;&quot;&amp; 
IF( LEN ( BillingStreet) = 0, &quot;Billing Street, &quot;,&quot;&quot;)&amp;&quot;&quot;&amp; 
IF( LEN ( BillingState) = 0, &quot;Billing State, &quot;,&quot;&quot; )&amp;&quot;&quot;&amp; 
IF( LEN ( BillingPostalCode) = 0, &quot;Billing Postal Code, &quot;,&quot;&quot;)&amp;&quot;&quot;&amp; 
